{
  "name": "mapi-transport",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "h:prepare": "husky install",
    "p:prepare": "panda codegen",
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "test": "playwright test",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "test:unit": "vitest",
    "lint": "prettier --plugin-search-dir . --check . && eslint .",
    "format": "prettier --plugin-search-dir . --write .",
    "pages:dev": "wrangler pages dev --compatibility-date=2023-09-16 --proxy 5173 -- pnpm run dev",
    "pages:deploy": "pnpm run build && wrangler pages deploy .svelte-kit/cloudflare"
  },
  "devDependencies": {
    "@pandacss/dev": "^0.15.5",
    "@playwright/test": "^1.28.1",
    "@sveltejs/adapter-auto": "^2.0.0",
    "@sveltejs/adapter-cloudflare": "^2.3.3",
    "@sveltejs/kit": "^1.5.0",
    "@typescript-eslint/eslint-plugin": "^5.45.0",
    "@typescript-eslint/parser": "^5.45.0",
    "@zerodevx/svelte-toast": "^0.9.5",
    "eslint": "^8.28.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-svelte": "^2.26.0",
    "husky": "^8.0.3",
    "lint-staged": "^14.0.1",
    "prettier": "^2.8.0",
    "prettier-plugin-svelte": "^2.8.1",
    "svelte": "^3.54.0",
    "svelte-check": "^3.0.1",
    "svelte-preprocess-import-assets": "^1.0.1",
    "tslib": "^2.4.1",
    "typescript": "^5.0.0",
    "vite": "^4.3.0",
    "vitest": "^0.34.4",
    "wrangler": "^3.8.0"
  },
  "lint-staged": {
    "*.{js,ts,json,md,css,html,svelte}": [
      "prettier --write"
    ]
  },
  "type": "module",
  "dependencies": {
    "@emailjs/nodejs": "^2.2.0",
    "svelte-media-queries": "^1.6.1"
  }
}
